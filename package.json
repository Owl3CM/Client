{
  "name": "eze-link",
  "version": "0.0.1",
  "description": "client",
  "main": "dist/index.js",
  "source": "src/index.ts",
  "private": false,
  "engines": {
    "node": ">=14"
  },
  "type": "module",
  "scripts": {
    "copy-config": "cp -r ./client.config.default.json ./dist/client.config.default.json",
    "copy-config-schema": " cp -r ./client.config.schema.json ./dist/client.config.schema.json",
    "build": "rm -rf dist && rollup -c --exports auto",
    "prod": "yarn build && node ./scripts/publish.js && git push",
    "start": "vite",
    "build-scriptsTs": "yarn tsc --build scripts.tsconfig.json",
    "delete-scriptsJS": "rm -rf ./scriptsJS",
    "build-client": "yarn build-scriptsTs && node ./dist/builder/buildClient.js",
    "build-swagger": "yarn build-scriptsTs && node ./dist/builder/buildFromSwagger.js",
    "test": "yarn build && yarn client",
    "postinstall": "node ./dist/builder/buildClient.js"
  },
  "bin": {
    "client": "./dist/buildClient.js"
  },
  "peerDependencies": {
    "react": ">=18.0.0",
    "react-dom": ">=18.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.19.3",
    "@rollup/plugin-commonjs": "^22.0.2",
    "@rollup/plugin-node-resolve": "^14.1.0",
    "@types/react": "^18.0.33",
    "@types/react-dom": "^18.0.6",
    "@types/react-router-dom": "^5.3.3",
    "@vitejs/plugin-react": "^3.1.0",
    "babel-loader": "^8.2.5",
    "chalk": "^5.3.0",
    "fs": "^0.0.1-security",
    "morabaa-utils": "^0.0.13",
    "postcss": "^8.4.16",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.10.0",
    "rollup": "^2.79.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.34.1",
    "ts-node": "^10.9.1",
    "typescript": "^4.8.4",
    "vite": "^4.2.1"
  },
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "eslintConfig": {
    "extends": [
      "react-app"
    ]
  },
  "keywords": [],
  "tags": [],
  "dependencies": {
    "morabaa-services": "^1.0.81",
    "node-fetch": "^3.3.2"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Owl3CM/Client.git"
  },
  "homepage": "https://github.com/Owl3CM/Client",
  "bugs": {
    "url": "https://github.com/Owl3CM/Client/issues"
  }
}
